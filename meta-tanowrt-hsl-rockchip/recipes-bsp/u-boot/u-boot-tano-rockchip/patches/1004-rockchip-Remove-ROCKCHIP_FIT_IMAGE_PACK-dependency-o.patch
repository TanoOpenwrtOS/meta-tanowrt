Date: Wed, 18 May 2022 18:09:14 +0300
Subject: [PATCH 1004/1013] rockchip: Remove ROCKCHIP_FIT_IMAGE_PACK dependency
 on ROCKCHIP_FIT_IMAGE

Signed-off-by: Anton Kikin <a.kikin@tano-systems.com>
---
 arch/arm/mach-rockchip/Kconfig | 1 -
 1 file changed, 1 deletion(-)

diff --git a/arch/arm/mach-rockchip/Kconfig b/arch/arm/mach-rockchip/Kconfig
index 8530e88..ceb4136 100644
--- a/arch/arm/mach-rockchip/Kconfig
+++ b/arch/arm/mach-rockchip/Kconfig
@@ -833,7 +833,6 @@ config ROCKCHIP_PRELOADER_SERIAL
 
 config ROCKCHIP_FIT_IMAGE_PACK
 	bool "Rockchip fit image pack of U-Boot and TEE"
-	depends on ROCKCHIP_FIT_IMAGE
 	default n
 	help
 	  This enable fit image pack of U-Boot and TEE, it's used for make.sh script.
