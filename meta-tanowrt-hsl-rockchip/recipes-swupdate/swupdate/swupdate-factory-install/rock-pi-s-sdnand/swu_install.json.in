{
	"global": {
		"last_action": "reboot",
		"last_action_delay": 5
	},
	"verify": {
		"blkdev_required": [
			"/dev/mmcblk0",
			"/dev/mmcblk1"
		]
	},
	"preinstall": {
		"mounts": [
			{
				"device": "/dev/mmcblk1p3",
				"target": "/mnt",
				"create_target": true,
				"fstype": "vfat"
			}
		],
		"blkdev_erase": [
			{ "device": "/dev/mmcblk0", "bs": 512, "count": 131072 }
		],
		"blkdev_parts": [
			{
				"device": "/dev/mmcblk0",
				"label": "gpt",
				"partitions": [
					{ "name": "boot",        "fstype": "",     "uuid": "@@TANOWRT_PARTUUID_BOOT@@",        "start": "8MiB",    "end": "16MiB"  },
					{ "name": "kernel_a",    "fstype": "ext4", "uuid": "@@TANOWRT_PARTUUID_KERNELDEV_A@@", "start": "16MiB",   "end": "64MiB"  },
					{ "name": "rootfs_a",    "fstype": "",     "uuid": "@@TANOWRT_PARTUUID_ROOTDEV_A@@",   "start": "64MiB",   "end": "360MiB" },
					{ "name": "kernel_b",    "fstype": "ext4", "uuid": "@@TANOWRT_PARTUUID_KERNELDEV_B@@", "start": "360MiB",  "end": "408MiB" },
					{ "name": "rootfs_b",    "fstype": "",     "uuid": "@@TANOWRT_PARTUUID_ROOTDEV_B@@",   "start": "408MiB",  "end": "704MiB" },
					{ "name": "rootfs_data", "fstype": "ext4", "uuid": "@@TANOWRT_PARTUUID_OVERLAY@@",     "start": "704MiB",  "end": "100%"   }
				]
			}
		],
		"blkdev_mkfs": [
			{
				"device": "/dev/mmcblk0p6",
				"fstype": "ext4",
				"label": "rootfs_data"
			}
		],
		"blkdev_write": [
			{
				"device": "/dev/mmcblk0",
				"images": [
					{
						"offset": 4128768,
						"image_source": "local",
						"image_url": "/mnt/u-boot-initial-env.bin"
					}, {
						"offset": 4161536,
						"image_source": "local",
						"image_url": "/mnt/u-boot-initial-env.bin"
					}
				]
			}
		]
	},
	"install": {
		"images": [
			{
				"type": "swupdate",
				"image_source": "local",
				"image_url": "/mnt/swu_install.swu",
				"extra": "--no-bootloader-env",
				"dry_run": true,
				"selections": [
					"${SWU_SYSTEM_A_SELECTION}",
					"${SWU_SYSTEM_B_SELECTION}"
				]
			}
		]
	},
	"postinstall": {
		"postexecute": [
			{ "command": "fw_setenv swu_active 0" },
			{ "command": "fw_setenv swu_upgrade_available 0" },
			{ "command": "fw_setenv swu_bootcount 0" }
		]
	}
}
